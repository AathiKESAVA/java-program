import java.util.Scanner;

class Main {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       int set1 = 1;
       int set2 = 1;
       for(int i=1;i<=n;i++){
       System.out.print(set1+" ");
       set1 += set2;
       set2++;
       }
    }
}
