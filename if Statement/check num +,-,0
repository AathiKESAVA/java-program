package usage;
import java.util.Scanner;
public class main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if(a==0) {
			System.out.print("Zero");
		}
		else if(a>=1) {
			System.out.print("Positive number");
		}
	     else{
			System.out.print("Negative number");
		}
		

	}
